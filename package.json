  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luozhihua/sublproject/issues"
  },
  "homepage": "https://github.com/luozhihua/sublproject",
  "dependencies": {
    "async": "~0.9.0",
    "rc": "~0.5.0",
    "open": "~0.0.5",
    "optimist": "~0.6.1"
  }
