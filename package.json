{
  "name": "sublproject",
  "version": "1.1.1",
  "preferGlobal": "true",
  "description": "Init current nodejs project for sublime-text, for easily to open it.",
  "main": "index.js",
  "os":["darwin", "linux"],
  "bin": {
    "sublproject": "./bin/sublproject",
    "sublp": "./bin/sublproject",
    "stp": "./bin/sublproject"
  },
  "scripts": {
    "test": "NODE_ENV=test bash test/index.sh && NODE_ENV=test cd text && node main.js"
  },
  "repository": {
    "type": "git",
